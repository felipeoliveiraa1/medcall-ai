{
  "name": "@medcall/shared-types",
  "version": "1.0.0",
  "private": true,
  "description": "Shared TypeScript types and interfaces for MedCall",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "default": "./dist/api.js"
    },
    "./call": {
      "types": "./dist/call.d.ts",
      "default": "./dist/call.js"
    },
    "./transcription": {
      "types": "./dist/transcription.d.ts",
      "default": "./dist/transcription.js"
    },
    "./medical": {
      "types": "./dist/medical.d.ts",
      "default": "./dist/medical.js"
    }
  }
}